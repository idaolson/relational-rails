<%= link_to "Home", "/"%>
<%= link_to "Boba Shops", "/boba_shops"%>
<%= link_to "Drinks", "/drinks"%>
<%= link_to "Restaurants", "/restaurants"%>
<%= link_to "Pastas", "/pastas"%>

<h1><%= @boba_shop.name %>'s drinks</h1>
<%= link_to "Create Drink", "/boba_shops/#{@boba_shop.id}/drinks/new" %>

<% @drinks.each do |drink| %>
  <h3><%= drink.name %></h3>
  Drink ID: <%= drink.id %><br>
  Store ID: <%= drink.boba_shop_id %><br>
  Created at: <%= drink.created_at %><br>
  Updated at: <%= drink.updated_at %><br>
  In stock?: <%= drink.in_stock %><br>
  price: <%= drink.price %>
<% end %>
